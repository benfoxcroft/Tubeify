{
  "compilerOptions": {
    "target": "es2017",
    "module": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./dst",
    "moduleResolution": "node16",
    "allowSyntheticDefaultImports": true,
    "declaration": true
  }
}